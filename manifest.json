{
  "manifest_version": 2,
  "name": "wordle",
  "version": "1.0",
  "description": "wordle accelerator",
  "content_scripts": [
    {
      "matches": ["https://*.nytimes.com/*"],
      "js": ["solver.js", "answers.js", "content.js"]
    }
  ],
  "web_accessible_resources": ["words.json"],
  "permissions": ["activeTab"]
}
